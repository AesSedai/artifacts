{
    "name": "artifacts",
    "version": "0.0.1",
    "packageManager": "yarn@3.5.1",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "tsc && vite build",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist",
        "dev": "vite",
        "preview": "vite preview",
        "generate-client": "openapi-rq -i ./src/api/openapi.json -c axios -o src/api/artifacts",
        "download-assets": "tsx ./src/scripts/downloadAssets.ts"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "project": "./tsconfig.json"
        },
        "plugins": [
            "@typescript-eslint"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended",
            "plugin:import/typescript",
            "prettier"
        ],
        "rules": {
            "no-void": "off",
            "@typescript-eslint/no-dynamic-delete": "off",
            "@typescript-eslint/no-non-null-assertion": "off",
            "@typescript-eslint/no-unused-vars": "off",
            "@typescript-eslint/restrict-template-expressions": "off"
        },
        "root": true,
        "settings": {
            "import/parsers": {
                "@typescript-eslint/parser": [
                    ".ts",
                    ".tsx"
                ]
            },
            "import/resolver": {
                "typescript": true,
                "node": true
            }
        }
    },
    "dependencies": {
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "@mui/icons-material": "^6.0.1",
        "@mui/lab": "^6.0.0-beta.8",
        "@mui/material": "^6.0.1",
        "@tanstack/react-query": "^5.53.2",
        "axios": "^1.7.7",
        "dotenv": "^16.4.5",
        "envalid": "^8.0.0",
        "lodash-es": "^4.17.21",
        "luxon": "^3.5.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.26.1",
        "ts-toolbelt": "^9.6.0",
        "use-async-effect": "^2.2.7"
    },
    "devDependencies": {
        "@7nohe/openapi-react-query-codegen": "^1.6.0",
        "@tanstack/react-query-devtools": "^5.53.2",
        "@types/lodash-es": "^4.17.12",
        "@types/luxon": "^3.4.2",
        "@types/node": "^22.5.2",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/parser": "^5.0.1",
        "@vitejs/plugin-react": "^4.3.1",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-unused-imports": "^2.0.0",
        "gh-pages": "^6.1.1",
        "prettier": "^3.1.0",
        "prettier-eslint": "^16.1.2",
        "prettier-plugin-organize-imports": "^3.2.2",
        "rollup-plugin-visualizer": "^5.12.0",
        "ts-morph": "^23.0.0",
        "tsx": "^4.19.0",
        "typescript": "^5.5.4",
        "vite": "^5.4.2"
    }
}
